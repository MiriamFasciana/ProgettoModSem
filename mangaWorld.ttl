@prefix : <http://modSem/miriam/ontologies/2020/mangaWorld> .
@prefix db: <http://dbpedia.org/> .
@prefix dbr: <http://dbpedia.org/resource/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix time: <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl#> .
@prefix manga: <http://modSem/miriam/ontologies/2020/mangaWorld#> .
@base <http://modSem/miriam/ontologies/2020/mangaWorld> .

<http://modSem/miriam/ontologies/2020/mangaWorld> rdf:type owl:Ontology ;
                                                   owl:imports <http://www.ontologydesignpatterns.org/cp/owl/partof.owl> ,
                                                               <http://www.ontologydesignpatterns.org/cp/owl/timeinterval.owl> ;
                                                   <http://purl.org/dc/elements/1.1/creator> "Miriam Fasciana"@en ,
                                                                                             "Miriam Fasciana"@it ;
                                                   <http://purl.org/dc/elements/1.1/date> "2020-01-07"@en ,
                                                                                          "2020-01-07"@it ;
                                                   <http://purl.org/dc/elements/1.1/description> "Manga ontology specialized on categories"@en ,
                                                                                                 "Ontologia sul dominio dei manga focalizzata sulle categorie"@it ;
                                                   <http://purl.org/dc/elements/1.1/title> "Manga ontology"@en ,
                                                                                           "Ontologia sui manga"@it ;
                                                   <http://purl.org/vocab/vann/preferredNamespacePrefix> "manga" ;
                                                   <http://purl.org/vocab/vann/preferredNamespaceUri> "http://modSem/miriam/ontologies/2020/mangaWorld" ;
                                                   rdfs:label "MangaWorld"@en ,
                                                              "MondoManga"@it .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/date
<http://purl.org/dc/elements/1.1/date> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/description
<http://purl.org/dc/elements/1.1/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/title
<http://purl.org/dc/elements/1.1/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled
<http://swrl.stanford.edu/ontologies/3.3/swrla.owl#isRuleEnabled> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://modSem/miriam/ontologies/2020/mangaWorld#ambientata_in_contesto
manga:ambientata_in_contesto rdf:type owl:ObjectProperty ;
                             rdfs:domain manga:Categoria ;
                             rdfs:range manga:Contesto ;
                             rdfs:comment "(Manga's) Category has a plot set in a specific context"@en ,
                                          "La trama di una categoria (di manga) è ambientata in uno specifico contesto"@it ;
                             rdfs:label "Ambientata in contesto"@it ,
                                        "Set in context"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#disegna
manga:disegna rdf:type owl:ObjectProperty ;
              owl:inverseOf manga:disegnata_da ;
              rdfs:domain manga:Mangaka ;
              rdfs:range manga:Manga ;
              rdfs:comment "A mangaka draws a specific manga"@en ,
                           "Un mangaka disegna uno specifico manga"@it ;
              rdfs:label "Disegna"@it ,
                         "Draws"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#disegnata_da
manga:disegnata_da rdf:type owl:ObjectProperty ;
                   rdfs:domain manga:Manga ;
                   rdfs:range manga:Mangaka ;
                   rdfs:comment "A manga is drawn by a specific mangaka"@en ,
                                "Un manga è disegnato da uno specifico mangaka"@it ;
                   rdfs:label "Disegnato da"@it ,
                              "Drawn by"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#ha_categoria
manga:ha_categoria rdf:type owl:ObjectProperty ;
                   rdfs:domain manga:Manga ;
                   rdfs:range manga:Categoria ;
                   rdfs:comment "A manga belongs to a specific category"@en ,
                                "Un manga appartiene ad una categoria specifica"@it ;
                   rdfs:label "Ha categoria"@it ,
                              "Has category"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#ha_genere
manga:ha_genere rdf:type owl:ObjectProperty ;
                rdfs:domain manga:Categoria ;
                rdfs:range manga:Genere ;
                rdfs:comment "A category's plot belongs to a specific genre"@en ,
                             "La trama di una categoria appartiene ad uno specifico genere"@it ;
                rdfs:label "Ha genere"@it ,
                           "Has genre"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#has_chapter
manga:has_chapter rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                  owl:inverseOf manga:is_part_of_tankobon ;
                  rdfs:comment "A tankobon contains several chapters"@en ,
                               "Un tankobon contiene diversi capitoli"@it ;
                  rdfs:label "Ha capitolo"@it ,
                             "Has chapter"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#has_tankobon
manga:has_tankobon rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#hasPart> ;
                   owl:inverseOf manga:is_part_of_manga ;
                   rdfs:comment "A manga's series contains several tankobon"@en ,
                                "Una serie di manga è composta da diversi tankobon"@it ;
                   rdfs:label "Ha tankobon"@it ,
                              "Has tankobon"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#is_part_of_manga
manga:is_part_of_manga rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> ;
                       rdfs:domain manga:Tankobon ;
                       rdfs:range manga:Manga ;
                       rdfs:comment "A tankobon is a part of a specific manga"@en ,
                                    "Un tankobon fa parte di un certo manga"@it ;
                       rdfs:label "Is part of manga"@en ,
                                  "è parte del manga"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#is_part_of_tankobon
manga:is_part_of_tankobon rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf <http://www.ontologydesignpatterns.org/cp/owl/partof.owl#isPartOf> ;
                          rdfs:domain manga:Capitolo ;
                          rdfs:range manga:Tankobon ;
                          rdfs:comment "A chapter is contained in a specfic tankobon"@en ,
                                       "Un capitolo è contenuto in un certo tankobon"@it ;
                          rdfs:label "Is part of tankobon"@en ,
                                     "è parte del tankobon"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#magazine_ha_categoria
manga:magazine_ha_categoria rdf:type owl:ObjectProperty ,
                                     owl:FunctionalProperty ;
                            rdfs:domain manga:Manga_magazine ;
                            rdfs:range manga:Categoria ;
                            rdfs:comment "A magazine is specialized in a manga's category"@en ,
                                         "Una rivista è specializzata in una certa categoria di manga"@it ;
                            rdfs:label "Magazine has category"@en ,
                                       "Rivista ha categoria"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#manga_ha_contesto
manga:manga_ha_contesto rdf:type owl:ObjectProperty ;
                        rdfs:domain manga:Manga ;
                        rdfs:range manga:Contesto ;
                        owl:propertyChainAxiom ( manga:ha_categoria
                                                 manga:ambientata_in_contesto
                                               ) ;
                        rdfs:comment "A manga is set in a context"@en ,
                                     "Un manga è ambientato in un certo contesto"@it ;
                        rdfs:label "Manga ha contesto"@it ,
                                   "Manga has context"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#nazione_di_origine
manga:nazione_di_origine rdf:type owl:ObjectProperty ,
                                  owl:FunctionalProperty ;
                         rdfs:domain manga:Manga ;
                         rdfs:range manga:Nazione ;
                         rdfs:comment "A manga is originally published in a specific Nation"@en ,
                                      "Un manga viene pubblicato originariamente in una certa Nazione"@it ;
                         rdfs:label "Nazione di origine"@it ,
                                    "Original Nation"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#orientata_a_pubblico
manga:orientata_a_pubblico rdf:type owl:ObjectProperty ,
                                    owl:FunctionalProperty ;
                           rdfs:domain manga:Categoria ;
                           rdfs:range manga:Pubblico ;
                           rdfs:comment "A manga's category is aimed at a specific audience"@en ,
                                        "Una categoria è orientata verso uno specifico pubblico"@it ;
                           rdfs:label "Aimed at audience"@en ,
                                      "Orientata ad un pubblico"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#pubblicata_nel_periodo
manga:pubblicata_nel_periodo rdf:type owl:ObjectProperty ;
                             rdfs:domain manga:Manga ;
                             rdfs:range time:TimeInterval ;
                             rdfs:comment "A manga's publication takes a certain period of time"@en ,
                                          "La pubblicazione di un manga avviene un certo periodo di tempo"@it ;
                             rdfs:label "Pubblicata nel periodo"@it ,
                                        "Published in period"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#scritta_da
manga:scritta_da rdf:type owl:ObjectProperty ;
                 owl:inverseOf manga:scrive ;
                 rdfs:domain manga:Manga ;
                 rdfs:range manga:Mangaka ;
                 rdfs:comment "A manga is written by a specific mangaka"@en ,
                              "Un manga è scritto da uno specifico mangaka"@it ;
                 rdfs:label "Scritto da"@it ,
                            "Written by"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#scrive
manga:scrive rdf:type owl:ObjectProperty ;
             rdfs:domain manga:Mangaka ;
             rdfs:range manga:Manga ;
             rdfs:comment "A mangaka writes a specific manga"@en ,
                          "Un mangaka scrive uno specifico manga"@it ;
             rdfs:label "Scrive"@it ,
                        "Writes"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#serializzato_su
manga:serializzato_su rdf:type owl:ObjectProperty ,
                               owl:FunctionalProperty ;
                      rdfs:domain manga:Manga ;
                      rdfs:range manga:Manga_magazine ;
                      rdfs:comment "A manga is serialized in a specific magazine"@en ,
                                   "Un manga è serializzato su una specifica rivista"@it ;
                      rdfs:label "Is serialized in"@en ,
                                 "Serializzato su"@it .


#################################################################
#    Data properties
#################################################################

###  http://modSem/miriam/ontologies/2020/mangaWorld#nome_rivista
manga:nome_rivista rdf:type owl:DatatypeProperty ;
                   rdfs:domain manga:Manga_magazine ;
                   rdfs:range xsd:string ;
                   rdfs:comment "A manga magazine has a name"@en ,
                                "Una rivista di manga ha un nome"@it ;
                   rdfs:label "Magazine name"@en ,
                              "Nome rivista"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#numero_del_tankobon
manga:numero_del_tankobon rdf:type owl:DatatypeProperty ,
                                   owl:FunctionalProperty ;
                          rdfs:domain manga:Tankobon ;
                          rdfs:range xsd:integer ;
                          rdfs:comment "A tankobon is a manga's specific volume such as 'Judge 1'"@en ,
                                       "Un tankobon è un volume specifico del manga come ad esempio 'Judge 1'"@it ;
                          rdfs:label "Numero del tankobon"@it ,
                                     "Tankobon's number"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#numero_di_capitolo
manga:numero_di_capitolo rdf:type owl:DatatypeProperty ;
                         rdfs:domain manga:Capitolo ;
                         rdfs:range xsd:integer ;
                         rdfs:comment "A chapter has a number that follows the cronological order"@en ,
                                      "Un capitolo ha un suo numero che segue l'ordine cronologico"@it ;
                         rdfs:label "Chapter's number"@en ,
                                    "Numero del capitolo"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#numero_di_volumi
manga:numero_di_volumi rdf:type owl:DatatypeProperty ;
                       rdfs:domain manga:Manga ;
                       rdfs:range xsd:nonNegativeInteger ;
                       rdfs:comment "A manga contains a certain number of tankobon"@en ,
                                    "Un manga è composto da un certo numero di tankobon"@it ;
                       rdfs:label "Number of volumes"@en ,
                                  "Numero di volumi"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#sottotitolo
manga:sottotitolo rdf:type owl:DatatypeProperty ;
                  rdfs:domain manga:Manga ;
                  rdfs:range rdf:PlainLiteral ;
                  rdfs:comment "A manga could have a subtitle"@en ,
                               "Un manga può avere un sottotitolo"@it ;
                  rdfs:label "Sottotitolo"@it ,
                             "Subtitle"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#stato_in_patria
manga:stato_in_patria rdf:type owl:DatatypeProperty ;
                      rdfs:domain manga:Manga ;
                      rdfs:range [ rdf:type rdfs:Datatype ;
                                   owl:oneOf [ rdf:type rdf:List ;
                                               rdf:first "Completato" ;
                                               rdf:rest [ rdf:type rdf:List ;
                                                          rdf:first "In corso" ;
                                                          rdf:rest rdf:nil
                                                        ]
                                             ]
                                 ] ;
                      rdfs:comment "A manga's series could be terminated or continuing"@en ,
                                   "Una serie di manga può essere terminata oppure ancora in fase di pubblicazione"@it ;
                      rdfs:label "Stato in patria"@it ,
                                 "Status"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#titolo
manga:titolo rdf:type owl:DatatypeProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( manga:Capitolo
                                         manga:Manga
                                       )
                         ] ;
             rdfs:range rdf:PlainLiteral ;
             rdfs:comment "A manga could have different titles in different Nations, a chapter could have a title as well"@en ,
                          "Un manga ha titoli diversi a seconda della Nazione, anche un capitolo può avere un titolo"@it ;
             rdfs:label "Title"@en ,
                        "Titolo"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#titolo_originale
manga:titolo_originale rdf:type owl:DatatypeProperty ,
                                owl:FunctionalProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( manga:Capitolo
                                                   manga:Manga
                                                 )
                                   ] ;
                       rdfs:range rdf:PlainLiteral ;
                       rdfs:comment "A manga has a title in its original Nation"@en ,
                                    "Un manga ha un titolo nella sua Nazione di origine"@it ;
                       rdfs:label "Original title"@en ,
                                  "Titolo originale"@it .


#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Manga
<http://dbpedia.org/ontology/Manga> rdf:type owl:Class ;
                                    owl:equivalentClass manga:Manga .


###  http://https://www.wikidata.org/wiki/Q191633
<http://https://www.wikidata.org/wiki/Q191633> rdf:type owl:Class ;
                                               owl:equivalentClass manga:Mangaka ;
                                               rdfs:label "mangaka"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Adolescente
manga:Adolescente rdf:type owl:Class ;
                  rdfs:subClassOf manga:Pubblico ;
                  rdfs:comment "Il manga è rivolto agli adolescenti"@it ,
                               "The manga is aimed at the teenagers"@en ;
                  rdfs:label "Adolescente"@it ,
                             "Teenager"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Adolescente_femminile
manga:Adolescente_femminile rdf:type owl:Class ;
                            rdfs:subClassOf manga:Adolescente ;
                            owl:disjointWith manga:Adolescente_maschile ;
                            rdfs:comment "Il manga è rivolto alle ragazze adolescenti"@it ,
                                         "The manga is aimed at the teenager girls"@en ;
                            rdfs:label "Ragazze adolescenti"@it ,
                                       "Teenager girls"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Adolescente_maschile
manga:Adolescente_maschile rdf:type owl:Class ;
                           rdfs:subClassOf manga:Adolescente ;
                           rdfs:comment "Il manga è rivolto ai ragazzi adolescenti"@it ,
                                        "The manga is aimed at the teenager boys"@en ;
                           rdfs:label "Ragazzi adolescenti"@it ,
                                      "Teenager boys"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Adulto
manga:Adulto rdf:type owl:Class ;
             rdfs:subClassOf manga:Pubblico ;
             rdfs:comment "Il manga è rivolto agli adulti"@it ,
                          "The manga is aimed at the adults"@en ;
             rdfs:label "Adult"@en ,
                        "Adulto"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Amore_fisico
manga:Amore_fisico rdf:type owl:Class ;
                   rdfs:subClassOf manga:Sentimentale ;
                   owl:disjointWith manga:Amore_platonico ;
                   rdfs:comment "Plot about love in a physical way"@en ,
                                "Trama riguardante un amore sviluppato sul piano fisico"@it ;
                   rdfs:label "Amore fisico"@it ,
                              "Physical love"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Amore_omosessuale
manga:Amore_omosessuale rdf:type owl:Class ;
                        rdfs:subClassOf manga:Sentimentale ;
                        rdfs:comment "Plot about omosexual love"@en ,
                                     "Trama riguardante l'amore omosessuale"@it ;
                        rdfs:label "Amore omosessuale"@it ,
                                   "Omosexual love"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Amore_omosessuale_femminile
manga:Amore_omosessuale_femminile rdf:type owl:Class ;
                                  rdfs:subClassOf manga:Amore_omosessuale ;
                                  owl:disjointWith manga:Amore_omosessuale_maschile ;
                                  rdfs:comment "Plot about omosexual love between girls"@en ,
                                               "Trama riguardante l'amore omosessuale tra donne"@it ;
                                  rdfs:label "Amore omosessuale femminile"@it ,
                                             "Girls omosexual love"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Amore_omosessuale_maschile
manga:Amore_omosessuale_maschile rdf:type owl:Class ;
                                 rdfs:subClassOf manga:Amore_omosessuale ;
                                 rdfs:comment "Plot about omosexual love between boys"@en ,
                                              "Trama riguardante l'amore omosessuale tra uomini"@it ;
                                 rdfs:label "Amore omosessuale maschile"@it ,
                                            "Boys omosexual love"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Amore_platonico
manga:Amore_platonico rdf:type owl:Class ;
                      rdfs:subClassOf manga:Sentimentale ;
                      rdfs:comment "Plot about platonic love, without sexual references"@en ,
                                   "Trama riguardante l'amore sviluppato in modo platonico, senza riferimenti sessuali"@it ;
                      rdfs:label "Amore platonico"@it ,
                                 "Platonic love"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Azione
manga:Azione rdf:type owl:Class ;
             rdfs:subClassOf manga:Genere ;
             rdfs:comment "Genere di trama in cui i protagonisti devono affrontare una serie di sfide che richiedono coraggio e prodezza fisica"@it ,
                          "Plot's genre in which the main characters have to face challanges that requires bravery and physical prowess"@en ;
             rdfs:label "Action"@en ,
                        "Azione"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#BoysLove
manga:BoysLove rdf:type owl:Class ;
               rdfs:subClassOf manga:Categoria .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Capitolo
manga:Capitolo rdf:type owl:Class ;
               rdfs:comment "A manga's chapter"@en ,
                            "Capitolo di un manga"@it ;
               rdfs:label "Capitolo"@it ,
                          "Chapter"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Categoria
manga:Categoria rdf:type owl:Class ;
                rdfs:comment "Categoria di un manga come ad esempio shonen o shojo"@it ,
                             "Manga's category such as shonen or shojo"@en ;
                rdfs:label "Categoria"@it ,
                           "Category"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Contesto
manga:Contesto rdf:type owl:Class ;
               rdfs:comment "Caratteristica di opere letterarie che riguarda l'ambiente in cui si svolge la trama"@it ,
                            "The environment where the plot  is developed"@en ;
               rdfs:label "Contesto"@it ,
                          "Context"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Fantastico
manga:Fantastico rdf:type owl:Class ;
                 rdfs:subClassOf manga:Contesto ;
                 rdfs:comment "Plot set in a fantastic world"@en ,
                              "Trama ambientata in un mondo fantastico"@it ;
                 rdfs:label "Fantastic"@en ,
                            "Fantastico"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Genere
manga:Genere rdf:type owl:Class ;
             rdfs:comment "Literary genre"@en ,
                          "Tipo di trama di un'opera letteraria"@it ;
             rdfs:label "Genere"@it ,
                        "Genre"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Global_manga
manga:Global_manga rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty manga:nazione_di_origine ;
                                         owl:someValuesFrom manga:Nazione_non_giapponese
                                       ] ;
                   rdfs:subClassOf manga:Manga ;
                   rdfs:comment "Global Manga"@it ,
                                "Manga created outside Japan"@en ,
                                "Manga creati al di fuori del Giappone"@it ;
                   rdfs:label "Global manga"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Infantile
manga:Infantile rdf:type owl:Class ;
                rdfs:subClassOf manga:Pubblico ;
                rdfs:comment "Il manga è rivolto ai bambini"@it ,
                             "The manga is aimed at the children"@en ;
                rdfs:label "Children"@en ,
                           "Infantile"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Kodomo
manga:Kodomo rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty manga:orientata_a_pubblico ;
                                   owl:someValuesFrom manga:Infantile
                                 ] ;
             rdfs:subClassOf manga:Categoria ;
             rdfs:comment "Categoria di manga per bambini"@it ,
                          "Manga's category for children"@en ;
             rdfs:label "Kodomo"@en ,
                        "Kodomo"@it .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Magico
manga:Magico rdf:type owl:Class ;
             rdfs:subClassOf manga:Genere ;
             rdfs:comment "Plot about magic"@en ,
                          "Trama che riguarda la magia"@it ;
             rdfs:label "Magia"@it ,
                        "Magic"@en .


###  http://modSem/miriam/ontologies/2020/mangaWorld#Manga
manga:Manga rdf:type owl:Class ;
            rdfs:comment "Comic or graphic novel originating from Japan"@en ,
